{
  "AzureAdB2C": {
    "Domain": "<tenant-name>.onmicrosoft.com",
    "AzureADAppRolesProviderClientId": "<management-app-client-id>",
    "AzureADAppRolesProviderClientSecret": "<management-app-client-secret>",
    "Instance": "https://<tenant-name>.b2clogin.com/tfp/",
    "ClientId": "<web-app-client-id>",
    "CallbackPath": "/signin-oidc",
    "SignUpSignInPolicyId": "<web-app-policy-id-signupsignin>",
    "ResetPasswordPolicyId": "",
    "EditProfilePolicyId": ""
  },
  "AppRoles": {
    "UserAttributeName": "extension_AppRoles"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}